<div id="<%= dom_id client %>">
  <tr>
    <th scope="row"><%= client.id %></th>
    <td><%= client.name %></td>
    <td><%= client.age %></td>
    <td>
    <% if client.movies.last %>
      <%= link_to client.movies.last %>
    <% else client.movies.last%>
    <% end %>
    </td>
    <td><%= link_to "Ver", client, class: "btn btn-success mb-1" %></td>
    <td><%= link_to "Editar", edit_client_path(client), class: "btn btn-secondary mb-1" %></td>
    <td><%= button_to "Eliminar", client, method: :delete, class: "btn btn-danger mb-1" %></td>
  </tr>
</div>
